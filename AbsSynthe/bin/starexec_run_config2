#!/bin/bash
# This configuration allows to synthesize a controller
# $1 contains the input filename (the name of the AIGER-file).
COMMAND="./abssynthe $1 -o $1-result.aag"
$COMMAND
res=$?
if [[ $res == 10 ]]; then
    echo "REALIZABLE"
    cat "$1-result.aag"
else
    echo "UNREALIZABLE"
fi
exit $res
