#!/bin/bash
# This configuration allows to check (un)realizability only
# $1 contains the input filename (the name of the AIGER-file).
COMMAND="./abssynthe -a -c 1 -f 3 $1"
$COMMAND
res=$?
if [[ $res == 10 ]]; then
    echo "REALIZABLE"
elif [[ $res == 20 ]]; then
    echo "UNREALIZABLE"
else
    echo "UNKNOWN"
fi
exit $res
